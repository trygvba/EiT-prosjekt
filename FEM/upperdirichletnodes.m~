function [ nodes, uzi ] = upperdirichletnodes( displacement, p, u, boundary )



N =length(coords(:,1))
nodes=[];
j=1;
for i=boundary
    
    if  (p(i,3)+u(3*i))>=displacement)
        nodes(j)=i;
        j=j+1;
    end
    
end
nodes=nodes';
uzi = ones(length(nodes),1)*displacement - p(nodes,3);

end
