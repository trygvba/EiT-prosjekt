function [ Ux Uz Uy  ] = untitled3( ux,uy,uz,tetr,phys_group)

    i=phys_groups
   
    index=tetr(find(tetr(:,5)==i),(1:4));
    Ux=zeros(length(index(:,1)),length(ux(1,:)));
    Uy=zeros(length(index(:,1)),length(ux(1,:)));
    Uz=zeros(length(index(:,1)),length(ux(1,:)));
    Ux(1,:)=ux(index(:,1),:);
    Uy(2,:)=uy(index(:,2),:);
    Uz(3,:)=uz(index(:,3),:);
  
    
end

